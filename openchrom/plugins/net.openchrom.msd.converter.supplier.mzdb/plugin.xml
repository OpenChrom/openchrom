<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.chemclipse.msd.converter.chromatogramSupplier">
      <ChromatogramSupplier
            description="Reads and writes mzDB chromatograms"
            fileExtension=".mzDB"
            filterName="mzDB Chromatogram (*.mzDB)"
            id="net.openchrom.msd.converter.supplier.mzdb"
            importMagicNumberMatcher="net.openchrom.msd.converter.supplier.mzdb.converter.MagicNumberMatcher"
            importConverter="net.openchrom.msd.converter.supplier.mzdb.converter.ChromatogramImportConverter"
            exportConverter="net.openchrom.msd.converter.supplier.mzdb.converter.ChromatogramExportConverter"
            isExportable="true"
            isImportable="true">
      </ChromatogramSupplier>
   </extension>
</plugin>
